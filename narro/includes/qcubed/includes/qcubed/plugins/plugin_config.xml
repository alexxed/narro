<?xml version="1.0"?>
<plugins>
	

<plugin>
	<name><![CDATA[QFirebug]]></name>
	<description><![CDATA[Plugin that eases debugging of QCubed applications through integration with Firebug, an excellent Firefox extension for Javascript debugging.]]></description>
	<version>0.2</version>
	<platform_version>1.1</platform_version>
	<author name="Eduardo Garcia aka enzo" email="enzo [at] anexusit [dot] com"/>
	<files>
		<file type="misc_include" filename="includes/QFirebug.class.php" />
		<file type="misc_include" filename="includes/FirePHP.class.php" />
		<file type="misc_include" filename="includes/fb.php" />
		<file type="example" filename="example/qfirebug.php" />
		<file type="example" filename="example/qfirebug.tpl.php" />
		<file type="example" filename="example/qfirebug.png" />
	</files>
	<includes>
		<include_files classname="QFirebug" filename="includes/QFirebug.class.php" />
	</includes>
	<examples>
		<example filename="example/qfirebug.php" description="Introduction to Debugging with QFirebug" />
	</examples>
</plugin>

<plugin>
	<name><![CDATA[QFlot]]></name>
	<description><![CDATA[Use a JavaScript-based plotting library called Flot to create nice-looking charts. Based on jQuery.]]></description>
	<version>0.4</version>
	<platform_version>1.1</platform_version>
	<author name="Eduardo Garcia aka enzo" email="enzo [at] anexusit [dot] com"/>
	<files>
		<file type="control" filename="includes/QFlot.class.php" />
		<file type="control" filename="includes/QFlotSeries.class.php" />
		<file type="js" filename="js/jquery.flot.pack.js" />
		<file type="js" filename="js/excanvas.pack.js" />
		<file type="example" filename="example/qflot-bar.php" />
		<file type="example" filename="example/qflot-bar.tpl.php" />
		<file type="example" filename="example/qflot-timeseries.php" />
		<file type="example" filename="example/qflot-timeseries.tpl.php" />
	</files>
	<includes>
		<include_files classname="QFlot" filename="includes/QFlot.class.php" />
		<include_files classname="QFlotSeries" filename="includes/QFlotSeries.class.php" />
	</includes>
	<examples>
		<example filename="example/qflot-bar.php" description="QFlot: Display a bar chart" />
		<example filename="example/qflot-timeseries.php" description="QFlot: Display a time series chart" />
	</examples>
</plugin>

</plugins>
